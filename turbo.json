{
  "baseBranch": "origin/main",
  "pipeline": {
    "lint": {
      "outputs": []
    },
    "test": {
      "dependsOn": ["build"],
      "inputs": ["**/*.{ts,tsx,js,jsx}"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        ".next/**",
        "build/**",
        "node_modules/.cache/metro/**"
      ]
    },
    "dev:ios": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
